{
  "name": "BikeShare",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "node node_modules/jest/bin/jest.js --watch",
    "changed": "git diff-tree --no-commit-id --name-only -r --diff-filter=a HEAD $(git merge-base HEAD origin/master)",
    "changed:js": "npm run changed --silent | grep --color=never '.*\\.js$'",
    "clean": "rm -rf node_modules",
    "fix-githooks": "prepush -c package.json remove && node node_modules/husky/bin/install",
    "lint:all": "eslint . --fix",
    "lint:changed:js": "npm run changed:js --silent | xargs eslint",
    "precommit": "lint-staged",
    "prepush": "npm run lint:changed:js",
    "prettier": "prettier --single-quote --trailing-comma es5",
    "prettier:verify": "sh bin/prettier-verify.sh",
    "prettier:write": "npm run prettier -- --write \"static/**/*.js\"",
    "watch": "npm-watch",
    "subrepo-setup": "git clone https://github.com/ingydotnet/git-subrepo /usr/local/git-subrepo && echo 'source /usr/local/git-subrepo/.rc' >> ~/.bashrc"
  },
  "lint-staged": {
    "*.js": [
      "npm run --silent prettier:verify",
      "npm run --silent prettier -- --write",
      "eslint --fix",
      "git add"
    ]
  },
  "prepush": {
    "tasks": [
      "npm run lint-all"
    ]
  },
  "jest": {
    "preset": "jest-expo",
    "snapshotSerializers": [
      "./node_modules/jest-serializer-enzyme"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^6.1.0",
    "@hs/transmute": "^1.4.0",
    "@turf/turf": "^5.0.4",
    "babel": "^6.23.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "expo": "^21.0.3",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "keymirror": "^0.1.1",
    "moment": "^2.19.2",
    "moment-duration-format": "^1.3.0",
    "native-base": "^2.2.1",
    "prop-types": "^15.6.0",
    "react": "16.0.0-alpha.12",
    "react-native": "https://github.com/expo/react-native/archive/sdk-21.0.1.tar.gz",
    "react-native-scripts": "^1.5.0",
    "react-native-vector-icons": "^4.4.2",
    "react-redux": "^5.0.6",
    "react-router-native": "^4.2.0",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-persist": "^5.4.0",
    "redux-persist-transform-immutable": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "bikelock-api": "git://github.com/rit-bikeshare/bikelock-api.git"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "enzyme-to-json": "^3.0.0",
    "eslint": "^3.17.1",
    "eslint-config-hubspot": "^7.3.1",
    "eslint-plugin-react": "^6.10.1",
    "eslint-plugin-react-native": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-expo": "^22.0.0",
    "jest-serializer-enzyme": "^1.0.0",
    "lint-staged": "^4.3.0",
    "npm-watch": "^0.3.0",
    "prepush": "^3.1.11",
    "prettier": "^1.7.4",
    "react-addons-test-utils": "^15.6.2",
    "react-devtools": "^2.5.2",
    "react-dom": "^16.0.0-0"
  }
}
